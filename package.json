  "private": true,
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && vite build",
